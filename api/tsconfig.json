	"compilerOptions": {
		"target": "ESNext",
		"module": "CommonJS",
		"moduleResolution": "node",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"lib": ["esnext", "webworker"],
		"typeRoots": ["./node_modules/@types", "./node_modules/@cloudflare"]
	}
}
